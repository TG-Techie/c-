
def is_larger(
    _GreaterThanComparable_ should_larger,
    SomeType should_smaller,
) returns SomeType:
    return should_larger > should_smaller

def add_if_smaller(SomeType first, SomeType second) returns SomeOtherType:
    if is_larger(first, second):
        return second + first
    else:
        return first
